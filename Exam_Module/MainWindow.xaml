<Window x:Class="Exam_Module.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exam_Module"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Button Content="Exit" Click="Exit_Click" Height="30" Width="150" Grid.Row="1" HorizontalAlignment="Right" Margin="0 0 10 0"/>
        <WrapPanel Grid.Row="1">
        <Button Content="Users" Click="Users_Click" Height="30" Width="150" Grid.Row="1" HorizontalAlignment="Left" Margin="5"/>
        <Button Content="Products" Click="Products_Click" Height="30" Width="150" Grid.Row="1" HorizontalAlignment="Left" Margin="5"/>
        </WrapPanel>


        
        
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Grid.Row="0"/>
    </Grid>
</Window>
